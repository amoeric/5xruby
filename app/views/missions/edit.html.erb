<h1><%= t("mission.edit") %></h1>
<%= link_to t("return"), root_path %>
<%= simple_form_for([@user, @mission]) do |mission| %>
  <%= mission.input :name %>
  <%= mission.input :content %>
  <%= mission.input :start_time %>
  <%= mission.input :end_time %>
  <%= mission.submit t("submit") %>
<% end %>